


pipeline {
    agent any
    tools {
    	maven 'M3'
  	}
    stages {
    	stage('echo') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('build') {
            steps {
                sh 'mvn -f arith-api/pom.xml clean compile test package'
            }
        }
        
    }
} 